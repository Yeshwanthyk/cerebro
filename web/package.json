{
  "name": "cerebro-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "build:exe": "bun run ../scripts/build-executable.ts",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint -c oxlint.json src/",
    "lint:types": "eslint src/",
    "lint:all": "bun run lint && bun run lint:types",
    "lint:fix": "oxlint -c oxlint.json --fix src/ && eslint --fix src/",
    "check": "bun run lint:all && tsc --noEmit"
  },
  "dependencies": {
    "@pierre/precision-diffs": "^0.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "oxlint": "^1.31.0",
    "typescript": "^5.6.3"
  }
}
