{
  "name": "cerebro-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "echo 'Run: ./cerebro start --port 3000 (in another terminal)' && vite",
    "build": "biome check --write && tsc -b && vite build",
    "preview": "vite preview",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "typecheck": "tsc --noEmit",
    "check": "biome check src && tsc --noEmit"
  },
  "dependencies": {
    "@pierre/precision-diffs": "^0.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "typescript": "~5.6.3",
    "vite": "^6.0.1"
  }
}
